
public class House {
	
	private int nbWall;
	private String roofType;
	private int nbWindow;
	private String digicode;
	
	public void House(){
		nbWall = 4;
	}
	
	public House(int w, String r, int wd){
		nbWall = w;
		roofType = r;
		nbWindow = wd;
	}
	
	public void construction() {
		
	}
	
	public void controlDigicode(String type) {
		if (type == digicode) {
			System.out.println("Autorisé !"); //autorisation message
			}
	}
	
	

}
