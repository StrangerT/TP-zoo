package com.ib.zoo.entity;

public class CEO extends Personnel{
	private Zoo zoo;
	
	public CEO(){
		
	}
	
	public CEO(String fname, String lname, Zoo zoo){
		super(fname, lname);
		this.salary = 42000;
		this.zoo = zoo;
	}
	
	@Override
	public double getBonus() {
		// TODO Auto-generated method stub
		this.setBonus((this.zoo.getMonthlyIncome() - this.zoo.getMonthlyOutgo()) * 15 / 100);
		return this.bonus;
	}

}
