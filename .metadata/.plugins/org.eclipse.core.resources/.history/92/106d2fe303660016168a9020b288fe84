package com.ib.zoo.launch;

import com.ib.zoo.entity.Account;
import com.ib.zoo.entity.CEO;
import com.ib.zoo.entity.Zoo;

public class Launcher {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Account a = new Account();
		Zoo z = new Zoo();
		CEO ceo = new CEO("a", "b", z);
		z.setAccount(a);
		z.setCeo(ceo);
		
		ceo.recruit("Healer", "healer", "1");
		z.getPersonnels().get(0).setNbYears(1);
		System.out.println(z.getPersonnels().get(0).getSalary());
		System.out.println(ceo.getFirstname());
		a.toString();

	}

}
